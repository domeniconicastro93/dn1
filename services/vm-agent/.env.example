# Strike VM Agent Configuration

# Server port (default: 8787)
PORT=8787

# Strong random token for authentication (REQUIRED)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
VM_AGENT_TOKEN=your-strong-random-token-here

# Launch timeout in milliseconds (default: 5000)
LAUNCH_TIMEOUT_MS=5000
